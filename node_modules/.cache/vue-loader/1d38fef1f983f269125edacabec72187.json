word.value === '1234') {\n      localStorage.setItem('token', 'mock_token'); // 儲存 token\n      router.push('/dashboard'); // 跳轉到儀表板\n    } else {\n      errorMessage.value = '帳號或密碼錯誤';\n    }\n  } catch (error) {\n    errorMessage.value = '登入失敗，請稍後再試';\n  }\n};\n\ndefineProps(); // 使用 defineProps()\n\n// 明確返回變數\ndefineExpose({\n  login\n});\n</script>\n\n<style>\n.login-container { text-align: center; }\n.error { color: red; }\n</style>\n"]}]}